{
  "selectedAuthType": "gemini-api-key",
  "theme": "Default",
  "preferredEditor": "neovim",
  "mcpServers": {
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },

    "Git": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },

    "GitHub": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },

    "NixOS": {
      "command": "uvx",
      "args": ["mcp-nixos"]
    }
  }
}
